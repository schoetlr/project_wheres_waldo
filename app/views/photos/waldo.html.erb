

<div id="photo-container">
  <%= image_tag "waldo", size: "1100x1000", id: "photo" %>
</div>

<h1 id="score">Score: 0</h1>

<div id="top-scores">
  <h2>Top Ten All Time Scores</h2>
  <ul>
    <% @games.each do |game| %>
      <li><%= "#{game.user_name}, #{game.score}" %></li>
    <% end %>
  </ul>
</div>